TARGETNAME=lfsfiltctl
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=lfsfiltctl

C_DEFINES=/DUNICODE /D_UNICODE
INCLUDES=$(NDAS_INC_PATH);$(PSDK_INC_PATH)
RCOPTIONS=/i $(PSDK_INC_PATH)
USECXX_FLAG=/Tp
USER_C_FLAGS=/Wp64

!IF $(FREEBUILD)
USE_MSVCRT=1
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

MSC_WARNING_LEVEL=-W3 -WX
MSC_OPTIMIZATION=/Oitb2

# publish lib files to the project root
PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

SOURCES= lfsfiltctl.cpp
