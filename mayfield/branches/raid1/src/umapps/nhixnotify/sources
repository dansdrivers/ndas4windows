TARGETNAME=nhixnotify
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=nhixnotify

C_DEFINES=/DUNICODE /D_UNICODE
INCLUDES=$(NDAS_INC_PATH);$(NDAS_INC_PATH)\ndas;$(PSDK_INC_PATH)

RCOPTIONS=/i $(PSDK_INC_PATH)
USECXX_FLAG=/Tp
USER_C_FLAGS=/EHsc /Wp64

!IF $(FREEBUILD)
USE_MSVCRT=1
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

MSC_WARNING_LEVEL=-W3 -WX
MSC_OPTIMIZATION=/Oitb2

PRECOMPILED_INCLUDE=stdafx.h
PRECOMPILED_CXX=1

SOURCES=	\
	ndashixnotify.cpp \
	ndashixnotifyutil.cpp
	
# publish lib files to the project root
PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

