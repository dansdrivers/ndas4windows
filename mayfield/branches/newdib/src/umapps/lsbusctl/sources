TARGETNAME=lsbusctl
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=lsbusctl

INCLUDES=$(NDAS_INC_PATH)
C_DEFINES=-DUNICODE -D_UNICODE
USER_C_FLAGS=/Wp64

!IF $(FREEBUILD)
!ELSE
DEBUG_CRTS=1
!ENDIF

# publish lib files to the project root
PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

MSC_WARNING_LEVEL=-W3 -WX

SOURCES=lsbusctl.c
