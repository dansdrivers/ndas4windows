TARGETNAME=ndasid
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=ndasid

INCLUDES=$(NDAS_INC_PATH)

!IF $(FREEBUILD)
MSC_OPTIMIZATION=/Ox
!ENDIF

# to publish library
PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

MSC_WARNING_LEVEL=-W3 -WX

SOURCES=ndasid.cpp

