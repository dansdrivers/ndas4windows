TARGETNAME=ndasid
TARGETTYPE=PROGRAM
TARGETPATH=obj

USE_UNICODE=1

UMTYPE=console

!IFDEF USE_UNICODE
C_DEFINES=-DUNICODE -D_UNICODE
UMENTRY=wmain
!ELSE
UMENTRY=main
!ENDIF

INCLUDES=$(NDAS_INC_PATH);$(NDAS_PUBLIC_INC_PATH)

!IF $(FREEBUILD)
USE_MSVCRT=1
MSC_OPTIMIZATION=/Ox
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

TARGETLIBS=\
	$(NDAS_LIB_PATH)\libdes.lib \
	$(NDAS_LIB_PATH)\libscrc32.lib \
	$(NDAS_LIB_PATH)\libndasid.lib \
	$(NDAS_LIB_PATH)\libndasidenc.lib

SOURCES=main.c

